#include <stdio.h>
#include <string.h>
int dfa(char str[]) {
    int state = 0; 

    for (int i = 0; i < strlen(str); i++) {
        if (state == 0 && str[i] == '0') {
            state = 1;
        } else if (state == 1 && str[i] == '1') {
            state = 2; 
        } else {
            return 0;
        }
    }
    return state == 2;
}

int main() {
    char input[100];
    printf("Enter a string: ");
    scanf("%s", input);
    if (dfa(input)) {
        printf("Accepted\n");
    } else {
        printf("Rejected\n");
    }

    return 0;
}

